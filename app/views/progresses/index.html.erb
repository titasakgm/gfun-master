<% @progresses.each do |progress| %>
  <tr>
    <td><%= progress.projects_staff_id %></td>
    <td><%= progress.q1 %></td>
    <td><%= progress.q2 %></td>
    <td><%= progress.q3 %></td>
    <td><%= progress.q4 %></td>
    <td><%= link_to 'แสดง', progress %></td>
    <td><%= link_to 'แก้ไข', edit_progress_path(progress) %></td>
    <td><%= link_to 'ลบ', progress, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

