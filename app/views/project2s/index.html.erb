<h3 style="display:inline-block">รายการโครงการ</h3> | <%= link_to "สรุปโครงการ", report_target_path , :target => "_blank" %>
<br/>
<%= link_to 'เพิ่มโครงการใหม่', new_project2_path, :class => "new_project" %>
<% @project2s.each do |p| %>
