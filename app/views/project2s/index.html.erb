  <h2><%= p.title %></h2>
  <p>
  <span>
    <%= link_to 'ดูรายละเอียด', project2_path(p), :action => :show %>&nbsp;|&nbsp;
    <%= link_to 'แก้ไข', edit_project2_path(p) %>&nbsp;|&nbsp;
